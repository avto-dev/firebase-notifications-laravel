{
    "name": "avto-dev/firebase-notifications-laravel",
    "description": "Firebase cloud messaging channel for laravel",
    "homepage": "https://github.com/avto-dev/firebase-notifications-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": "jetexe2@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "google/apiclient": "^2.0",
        "guzzlehttp/guzzle": "^6.0",
        "illuminate/notifications": ">=5.3 <5.8",
        "illuminate/support": ">=5.3 <5.8"
    },
    "require-dev": {
        "avto-dev/dev-tools": "^1.5",
        "laravel/laravel": ">=5.3 <5.8",
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "~5.4 || ^6.5 || ~7.0 <7.5.0"
    },
    "autoload": {
        "psr-4": {
            "AvtoDev\\FirebaseCloudMessaging\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AvtoDev\\FirebaseCloudMessaging\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "@php ./vendor/bin/phpunit --no-coverage",
        "test-cover": "@php ./vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AvtoDev\\FirebaseCloudMessaging\\FcmServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
