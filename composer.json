{
    "name": "avto-dev/firebase-notifications-laravel",
    "description": "Some package description goes here",
    "keywords": [
        "some",
        "keywords",
        "goes",
        "here"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "avto-dev",
            "homepage": "https://github.com/avto-dev"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "google/apiclient": "^2.2",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/notifications": ">=5.4 <5.8",
        "illuminate/support": ">=5.4.0 <5.8.0",
        "tarampampam/wrappers-php": "^1.4"
    },
    "require-dev": {
        "avto-dev/dev-tools": "^1.9",
        "laravel/laravel": ">=5.4.0 <5.8.0",
        "mockery/mockery": "0.9.* || ~1.0",
        "phpstan/phpstan": "~0.9 || ^0.10",
        "phpunit/phpunit": "~5.7.10 || ^6.4 || ^7.0",
        "symfony/var-dumper": "~3.2 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "AvtoDev\\FirebaseNotificationsChannel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AvtoDev\\FirebaseNotificationsChannel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "@php ./vendor/bin/phpunit --no-coverage",
        "test-cover": "@php ./vendor/bin/phpunit --coverage-html='./coverage/html'",
        "phpstan": "@php ./vendor/bin/phpstan analyze --no-progress --ansi --level=max ./src"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AvtoDev\\FirebaseNotificationsChannel\\FcmServiceProvider"
            ]
        }
    },
    "support": {
        "issues": "https://github.com/avto-dev/firebase-notifications-laravel/issues",
        "source": "https://github.com/avto-dev/firebase-notifications-laravel"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
